<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/tremblex/blex-docker-templates/master/tremblex/nexus-web.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>Tools:</Category>
  <Name>nexus-web</Name>
  <Description>
    Nexus Web Description
  </Description>
  <Overview>
    Nexus Web Overview
  </Overview>
  <Support>https://github.com/Tremblex/</Support>
  <ReadMe>https://github.com/Tremblex/blex-nexus</ReadMe>
  <Registry>https://ghcr.io/tremblex/nexus-web:latest</Registry>
  <GitHub>https://github.com/binhex/arch-syncthing</GitHub>
  <Repository>ghcr.io/tremblex/nexus-web</Repository>
  <Project>https://github.com/Tremblex/blex-nexus</Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>5050</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
	<Variable>
      <Name>NEXUS_API_URL</Name>
      <Value>http://nexus-api-staging:5000/</Value>
    </Variable>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/config</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:80]</WebUI>
  <Icon>https://avatars.githubusercontent.com/u/5077439?v=4</Icon>
  <ExtraParams></ExtraParams>
</Containers>